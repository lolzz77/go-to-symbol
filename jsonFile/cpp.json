{
    "function" : {
        "myComment" : "",
        "whole" : ["\\b(?:void|int|char|float|double|long|short)\\s+([a-zA-Z_][a-zA-Z_0-9]*)\\s*\\([^)]*\\)\\s*\\{", "g"],
        "opening" : ["{", "}"],
        "before" : ["("]
    },
    "class" : {
        "myComment" : "",
        "whole" : ["^\\s*class\\s+\\w+\\s*\\{", "gm"],
        "opening" : ["{", "}"],
        "before" : ["("]
    },
    "struct" : {
        "myComment" : "",
        "whole" : ["struct\\s+[^{}]*\\{", "g"],
        "opening" : ["{", "}"],
        "before" : ["{"]
    },
    "enum" : {
        "myComment" : "",
        "whole" : ["enum\\s+[^{}]*\\{", "g"],
        "opening" : ["{", "}"],
        "before" : ["{"]
    },
    "comment" : {
        "myComment" : "",
        "whole" : ["\\/\\/.*|\\/\\*[^*]*\\*+(?:[^\\/*][^*]*\\*+)*\\/", "g"],
        "opening" : ["", ""],
        "before" : [""]
    },
    "macro" : {
        "myComment" : "",
        "whole" : ["#define\\s+([^\\n]+)\\n", "g"],
        "opening" : ["", ""],
        "after" : ["define"]
    },
    "global" : {
        "myComment" : "match 1 newline, then match anything except ';' & newline, at least one. Then match ';'",
        "whole" : ["\\n[^;\\n]+;", "g"],
        "opening" : ["", ""],
        "before" : ["=", ";"]
    }
}